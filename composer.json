{
    "name": "uyanik13/sweet-alert",
    "type": "library",
    "description": "A BEAUTIFUL, RESPONSIVE, CUSTOMIZABLE, ACCESSIBLE (WAI-ARIA) REPLACEMENT FOR JAVASCRIPT'S POPUP BOXES FOR LARAVEL BY Ogur Uyanik",
    "keywords": [
        "laravel",
        "laravel-package",
        "sweet-alert2",
        "sweet-alert",
        "notifier",
        "toast",
        "noty",
        "alert"
    ],
    "homepage": "https://github.com/DRO/sweet-alert",
    "license": "MIT",
    "authors": [{
        "name": "Ogur Uyanik",
        "email": "ogur.uyanik@gmail.com",
        "homepage": "https://dijitalreklam.org",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^5.4|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DRO\\SweetAlert\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "DRO\\SweetAlert\\SweetAlertServiceProvider"
            ],
            "aliases": {
                "Alert": "DRO\\SweetAlert\\Facades\\Alert"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}